{
    "compilerOptions": {
      "target": "es2015",
      "lib": ["DOM","es2015"],
      "types": ["reflect-metadata"],
      "module": "commonjs",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "declaration": false
    },
    "exclude": ["node_modules"]
  }
  